@model GestaodeFrota.Models.Multa

@{
    ViewData["Title"] = "Multa";
}

<h2>@ViewData["Title"]</h2>

<h4>Cadastro de Multa</h4>
<hr />

<div class="row">
    <div class="col-4">
        <form asp-action="Create">
            <div class="form-group">
                <label asp-for="LocalMulta" class="control-label"></label>
                <input asp-for="LocalMulta" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="DataMulta" class="control-label"></label>
                <input asp-for="DataMulta" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Gravidade" class="control-label"></label>
                <input asp-for="Gravidade" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Pontos" class="control-label"></label>
                <input asp-for="Pontos" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="CodIfracao" class="control-label"></label>
                <input asp-for="CodIfracao" class="form-control" />
            </div>
            <div class="form-group">
                <label asp-for="Valor" class="control-label"></label>
                <input asp-for="Valor" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" value="Criar" class="btn btn-danger" />
            </div>
        </form>
    </div>
</div>